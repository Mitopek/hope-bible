{
  "compilerOptions": {
    "skipLibCheck": true,
    "module": "commonjs",
    "target": "es2016",
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "types": ["@types/express"],
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules"]
}
